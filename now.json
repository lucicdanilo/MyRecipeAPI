{
  "version": 2, 
  "builds": [{ "src": "index.js", "use": "@now/node-server"}],
  "routes": [
    {
      "src": "/api/user/register",
      "dest": "auth.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/user/postrecipe",
      "dest": "auth.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/user/getrecipe",
      "dest": "auth.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/user/getid",
      "dest": "auth.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/user/login",
      "dest": "auth.js",
      "methods": ["POST"]
    }
  ]
}
